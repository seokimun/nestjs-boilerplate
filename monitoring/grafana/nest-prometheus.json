{
  "uid": "nest-prometheus",
  "title": "NestJS Prometheus Overview",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "type": "timeseries",
      "title": "Process CPU (seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "process_cpu_user_seconds_total",
          "legendFormat": "user",
          "refId": "A"
        },
        {
          "expr": "process_cpu_system_seconds_total",
          "legendFormat": "system",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Memory RSS (bytes)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "rss",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Heap Total / Used (bytes)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "nodejs_heap_size_total_bytes",
          "legendFormat": "heap_total",
          "refId": "A"
        },
        {
          "expr": "nodejs_heap_size_used_bytes",
          "legendFormat": "heap_used",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Event Loop Lag (seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "nodejs_eventloop_lag_seconds",
          "legendFormat": "lag",
          "refId": "A"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Active Handles / Requests",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "nodejs_active_handles_total",
          "legendFormat": "handles",
          "refId": "A"
        },
        {
          "expr": "nodejs_active_requests_total",
          "legendFormat": "requests",
          "refId": "B"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 24,
        "h": 8
      }
    }
  ]
}
